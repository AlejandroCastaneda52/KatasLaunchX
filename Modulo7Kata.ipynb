    "new_planet = input(\"Ingresa el nombre del planeta\")\n",
    "planets = []\n",
    "while new_planet.lower() != 'done':\n",
    "    planets.append(new_planet)\n",
    "    new_planet = input(\"Ingresa otro planeta o teclea 'Done' para finalizar el ciclo\")\n",
    "\n",
    "for planeta in planets:\n",
    "    print(planeta)\n",
    "\n",
    "\n"
   ]
